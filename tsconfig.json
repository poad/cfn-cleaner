{
  "compilerOptions": {
    "target": "esnext",
    "lib": [],
    "module": "esnext",
    "moduleResolution": "bundler",
    "rootDirs": [ "src" ],
    "declaration": true,
    "declarationMap": false,
    "outDir": "./bin",
    "inlineSourceMap": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "alwaysStrict": true
  },
  "exclude": [
    "vite.config.ts",
    "__tests__",
    "bin"
  ]

}
